	<xsd:attributeGroup ref="SVG.XLink.attrib" />
-->
	</xsd:attributeGroup>
	<xsd:attributeGroup name="SVG.AnimationAttribute.attrib">
		<xsd:attribute name="attributeName" use="required" />
		<xsd:attribute name="attributeType" />
	</xsd:attributeGroup>
