					<xsd:enumeration value="rl" />
					<xsd:enumeration value="tb" />
					<xsd:enumeration value="inherit" />
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="SVG.TextContent.attrib">
		<xsd:attribute name="alignment-baseline">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="auto" />
					<xsd:enumeration value="baseline" />
					<xsd:enumeration value="before-edge" />
					<xsd:enumeration value="text-before-edge" />
					<xsd:enumeration value="middle" />
					<xsd:enumeration value="central" />
					<xsd:enumeration value="after-edge" />
					<xsd:enumeration value="text-after-edge" />
					<xsd:enumeration value="ideographic" />
					<xsd:enumeration value="alphabetic" />
					<xsd:enumeration value="hanging" />
					<xsd:enumeration value="mathematical" />
					<xsd:enumeration value="inherit" />
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="baseline-shift" />
