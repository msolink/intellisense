			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ltr" />
					<xsd:enumeration value="rtl" />
					<xsd:enumeration value="inherit" />
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="dominant-baseline">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="auto" />
					<xsd:enumeration value="use-script" />
					<xsd:enumeration value="no-change" />
					<xsd:enumeration value="reset-size" />
					<xsd:enumeration value="ideographic" />
					<xsd:enumeration value="alphabetic" />
					<xsd:enumeration value="hanging" />
					<xsd:enumeration value="mathematical" />
					<xsd:enumeration value="central" />
					<xsd:enumeration value="middle" />
					<xsd:enumeration value="text-after-edge" />
					<xsd:enumeration value="text-before-edge" />
					<xsd:enumeration value="inherit" />
